    "import qiskit\n",
    "\n",
    "qc = qiskit.QuantumCircuit(2)\n",
    "\n",
    "qc.crz(0.999, 0, 1)\n",
    "qc_tran = qiskit.transpile(qc, basis_gates=['h', 's', 'cx', 'rx', 'ry', 'rz'], optimization_level=3)\n",
    "qc_tran.draw(output='mpl')"
   ]
